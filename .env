# Slack channel ID (something like C05H9KYLPFX)
SLACK_CHANNEL_ID=
# If enabled, winners with most days not logged will be displayed in the channel with SLACK_CHANNEL_ID
ENABLE_WINNERS=true
# Minimum days not logged to be part of the "winners" list. :D
WINNERS_MIN_DAYS=3
# Same email as the account from which the API token was generated.
JIRA_EMAIL=
JIRA_API_TOKEN=
# No slash!
JIRA_BASE_URL=
# Generated from the Tempo app in the JIRA (settings -> tokens...)
TEMPO_API_TOKEN=
# No slash!
TEMPO_BASE_URL=https://api.tempo.io
# The main job token, should start with something like "xoxb-...."
SLACK_BOT_TOKEN=
# Comma separated emails.
EMAIL_LIST=""
PORT=80
DEBUG=true
