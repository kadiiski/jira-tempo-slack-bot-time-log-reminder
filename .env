TEMPO_API_TOKEN=
# No slash!
TEMPO_BASE_URL=https://api.tempo.io
# The main job token, should start with something like "xoxb-...."
SLACK_BOT_TOKEN=
# Comma separated emails.
EMAIL_LIST=""
PORT=80
DEBUG=false
TEST_MODE=false
MINIMUM_HOURS=7
CRON_TIME="0 15 * * 1-5"

